/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "@components": [
        "./src/app/components"
      ],
      "@components/*": [
        "./src/app/components/*"
      ],
      "@layouts": [
        "./src/app/layouts"
      ],
      "@layouts/*": [
        "./src/app/layouts/*"
      ],
      "@models": [
        "./src/app/models"
      ],
      "@models/*": [
        "./src/app/models/*"
      ],
      "@services": [
        "./src/app/services"
      ],
      "@services/*": [
        "./src/app/services/*"
      ],
      "@graphQL": [
        "./src/app/graphQL"
      ],
      "@graphQL/*": [
        "./src/app/graphQL/*"
      ]
    },
    "types": []
  },
  "extends": "./tsconfig.json",
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}